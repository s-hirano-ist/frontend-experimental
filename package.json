{
  "name": "frontend-experimental",
  "version": "1.0.0",
  "description": "experimental frontend project",
  "author": "s-hirano-ist",
  "packageManager": "pnpm@8.14.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "fmt": "prettier --check './src/**/*.{js,ts,tsx}'",
    "fmt:fix": "prettier --write './src/**/*.{js,ts,tsx}'",
    "prepare": "husky install",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:rest": "rm -rf src/types/generated && openapi2aspida -i src/types/swagger.json -o src/types/generated"
  },
  "lint-staged": {
    "**/*.{js, jsx, ts, tsx}": "pnpm fmt",
    "src/**/*.{js, jsx, ts, tsx}": "pnpm lint"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@mui/x-data-grid": "^6.18.6",
    "@parcel/watcher": "^2.3.0",
    "@prisma/client": "^5.7.1",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^5.17.1",
    "@tanstack/react-query-devtools": "^5.17.1",
    "@vercel/analytics": "^1.1.1",
    "@vercel/blob": "^0.16.1",
    "aspida": "^1.14.0",
    "axios": "^1.6.4",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.1.1",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "next": "14.0.3",
    "next-auth": "^4.24.5",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "recoil": "^0.7.7",
    "unified": "^11.0.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.1.0",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.23.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "rehype": "^13.0.1",
    "rehype-external-links": "^3.0.0",
    "rehype-highlight": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
