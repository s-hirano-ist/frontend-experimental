{
  "name": "frontend-experimental",
  "version": "1.1.0",
  "description": "experimental frontend project",
  "author": "s-hirano-ist",
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "fmt": "prettier --check './src/**/*.{js,ts,tsx}'",
    "fmt:fix": "prettier --write './src/**/*.{js,ts,tsx}'",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:rest": "rm -rf src/types/generated && openapi2aspida -i src/types/swagger.json -o src/types/generated"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.10.5",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.6.0",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/x-data-grid": "^7.7.0",
    "@parcel/watcher": "^2.4.1",
    "@prisma/client": "^5.15.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-devtools": "^5.45.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/blob": "^0.23.3",
    "aspida": "^1.14.0",
    "axios": "^1.7.2",
    "graphql": "^16.8.2",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.4.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "next": "14.2.4",
    "next-auth": "^4.24.7",
    "notistack": "^3.0.1",
    "react": "^18.3.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.0",
    "recoil": "^0.7.7",
    "unified": "^11.0.4",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@types/node": "^20.14.5",
    "@types/react": "^18.3.3",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "npm-run-all2": "^6.2.0",
